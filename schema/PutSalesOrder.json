{
	"name": "NAV 2009 Frederick Goldman - PUT SalesOrder Schema",
	"schemaDef": {
		"$schema": "http://json-schema.org/draft-04/schema#",
		"title": "NAV 2009 Frederick Goldman - PUT SalesOrder Schema",
		"type": "object",
		"properties": {
			"Order_No": {
				"type": "string"
			},
			"Sell_to_No": {
				"type": "string"
			},
			"Sell_to_Customer_Name": {
				"type": "string"
			},
			"Sell_to_Email": {
				"type": "string"
			},
			"Ship_to_Name": {
				"type": "string"
			},
			"Ship_to_Address": {
				"type": "string"
			},
			"Ship_to_City": {
				"type": "string"
			},
			"Ship_to_Post_County": {
				"type": "string"
			},
			"Ship_to_Post_Code": {
				"type": "string"
			},
			"Shipment_Method_Code": {
				"type": "string"
			},
			"orderStatus": {
				"type": "string",
				"enum": [
					"Open",
					"Released",
					"Pending_Approval",
					"Pending_Prepayment"
				]
			},
			"SalesLine": {
				"type": "array",
				"minItems": 1,
				"items": {
					"$ref": "#/definitions/SalesLineObject"
				}
			}
		},
		"required": [
			"Order_No",
			"Sell_to_No",
			"Sell_to_Customer_Name",
			"Sell_to_Email",
			"Ship_to_Name",
			"Ship_to_Address",
			"Ship_to_City",
			"Ship_to_Post_County",
			"Ship_to_Post_Code",
			"Shipment_Method_Code",
			"SalesLine"
		],
		"definitions": {
			"SalesOrderObject": {
				"type": "object"
			},
			"SalesLineObject": {
				"type": "object",
				"properties": {
					"ItemNo_Size": {
						"type": "string"
					},
					"Quantity": {
						"type": "number"
					},
					"Price": {
						"type": "number"
					},
					"Personalizations_Font": {
						"type": "string"
					},
					"Personalizations_Symbol": {
						"type": "string"
					},
					"Personalizations_Message": {
						"type": "string"
					}
				},
				"required": [
					"ItemNo_Size",
					"Quantity",
					"Price"
				]
			},
			"customer": {
				"title": "address",
				"type": "object",
				"properties": {
					"E_Mail": {
						"title": "E_Mail",
						"description": "The customer's email address.",
						"type": [
							"string"
						]
					},
					"Name": {
						"title": "Name",
						"description": "The customer's name.",
						"type": "string"
					},
					"Phone_No": {
						"title": "Phone_No",
						"description": "The customer's phone number.",
						"type": [
							"string"
						]
					},
					"Address": {
						"title": "Address",
						"description": "The customer's mailing address # 1.",
						"type": "string"
					},
					"Address_2": {
						"title": "Address_2",
						"description": "An additional field for the customer's mailing address.",
						"type": [
							"string",
							"null"
						]
					},
					"city": {
						"title": "city",
						"description": " The customer's city.",
						"type": "string"
					},
					"County": {
						"title": "County",
						"description": "The customer's province or state name.",
						"type": [
							"string"
						]
					},
					"Postal_Code": {
						"title": "Postal_Code",
						"description": "The customer's zip or postal code.",
						"type": "string"
					}
				},
				"required": [
					"E_Mail",
					"Name",
					"Phone_No",
					"Address",
					"Address_2",
					"City",
					"County",
					"Postal_Code"
				],
				"additionalProperties": false
			}
		}
	},
	"tags": [
		"Put",
		"SalesOrder"
	],
	"audit": {}
}
